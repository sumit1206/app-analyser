                    }
                    
//                    if(res.message['mute_locations']) {
//                        var mute_range_colors = ['#81B622', '#FAD02C', '#0476D0', '#FF8300', '#F83839'];
//                        $.each(res.message['mute_locations'], function( index, value ) {
//                            loadMarkersGroup(value, mute_range_colors[index], 'mute');
